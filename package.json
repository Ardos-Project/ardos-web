{
  "name": "ardos-web",
  "version": "1.0.0",
  "author": "Ardos Team",
  "description": "Ardos Web Panel",
  "homepage": "https://ardos-project.github.io/ardos-web/",
  "scripts": {
    "start": "cross-env NODE_ENV=development DEV_LOG=true webpack serve --mode=development --stats-error-details",
    "build": "cross-env NODE_ENV=production webpack --mode=production",
    "format": "prettier --write ./src/**/*.{ts,tsx}",
    "format:check": "prettier --check ./src/**/*.{ts,tsx}",
    "predeploy": "cross-env NODE_ENV=production ASSET_PATH=\"https://ardos-project.github.io/ardos-web/\" webpack --mode=production",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.3",
    "react": "^18.2.0",
    "react-code-blocks": "^0.1.6",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.23.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.24",
    "@types/react-helmet": "^6.1.6",
    "@types/react-router-dom": "^5.3.3",
    "autoprefixer": "^10.4.14",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.1",
    "esbuild-loader": "^4.0.2",
    "gh-pages": "^6.1.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss": "^8.4.21",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.1.0",
    "process": "^0.11.10",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "webpack": "^5.78.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^5.0.4"
  },
  "prettier": "./prettier-config.json"
}
